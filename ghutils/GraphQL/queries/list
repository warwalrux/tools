query { 
  search(
    type:REPOSITORY,
    query:"is:public sort:updated",
    first: 50
  ) {
    repos: edges{
      repo:node{
        ... on Repository {
          url,
          name,
          createdAt,
          updatedAt
        }
      }
    },
    pageInfo {
      endCursor
      hasNextPage
    }
  }
}
